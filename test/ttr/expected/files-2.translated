        UID=`id -u`
    fi
    if [ -d /run/user/$UID ]; then
        export WORKING_DIR=`mktemp -d -p /run/user/$UID`
    elif [ -d /dev/shm ]; then
        export WORKING_DIR=`mktemp -d -p /dev/shm`
    else
        export WORKING_DIR=`mktemp -d`
    fi
else
    export WORKING_DIR=$WORKING_DIR/working-$$
    mkdir $WORKING_DIR
fi
